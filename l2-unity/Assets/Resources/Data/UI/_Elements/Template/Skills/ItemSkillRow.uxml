            </engine:GroupBox>
            <engine:GroupBox name="GrowBoxText" style="margin-top: 0.3%; margin-right: 0; margin-bottom: 0; margin-left: 1%; padding-top: 0; padding-bottom: 0; padding-left: 0;">
